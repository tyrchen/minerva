    "components": {
        "schemas": {
            "CreateDatasetRequestContent": {
                "type": "object",
                "properties": {
                    "name": {
                        "type": "string"
                    },
                    "sql": {
                        "type": "string"
                    }
                },
                "required": [
                    "name",
                    "sql"
                ]
            },
            "DatasetField": {
                "type": "object",
                "properties": {
                    "name": {
                        "type": "string"
                    },
                    "type": {
                        "type": "string"
                    },
                    "nullable": {
                        "type": "boolean"
                    }
                },
                "required": [
                    "name",
                    "nullable",
                    "type"
                ]
            },
            "DatasetInfo": {
                "type": "object",
                "properties": {
                    "name": {
                        "type": "string"
                    },
                    "tableName": {
                        "type": "string"
                    },
                    "lastModified": {
                        "type": "number",
                        "format": "double"
                    },
                    "size": {
                        "type": "integer",
                        "format": "int64"
                    },
                    "fields": {
                        "type": "array",
                        "items": {
                            "$ref": "#/components/schemas/DatasetField"
                        }
                    }
                },
                "required": [
                    "fields",
                    "lastModified",
                    "name",
                    "size",
                    "tableName"
                ]
            },
            "ErrorCode": {
                "type": "string",
                "enum": [
                    "infer",
                    "network",
                    "database",
                    "unknown"
                ]
            },
            "ForbiddenErrorResponseContent": {
                "type": "object",
                "description": "Forbidden error.",
                "properties": {
                    "message": {
                        "type": "string"
                    }
                },
                "required": [
                    "message"
                ]
            },
            "GetDatasetResponseContent": {
                "type": "object",
                "properties": {
                    "name": {
                        "type": "string"
                    },
                    "tableName": {
                        "type": "string"
                    },
                    "lastModified": {
                        "type": "number",
                        "format": "double"
                    },
                    "size": {
                        "type": "integer",
                        "format": "int64"
                    },
                    "fields": {
                        "type": "array",
                        "items": {
                            "$ref": "#/components/schemas/DatasetField"
                        }
                    }
                },
                "required": [
                    "fields",
                    "lastModified",
                    "name",
                    "size",
                    "tableName"
                ]
            },
            "HealthCheckResponseContent": {
                "type": "object",
                "properties": {
                    "message": {
                        "type": "string"
                    }
                },
                "required": [
                    "message"
                ]
            },
            "ListDatasetResponseContent": {
                "type": "object",
                "properties": {
                    "items": {
                        "type": "array",
                        "items": {
                            "$ref": "#/components/schemas/DatasetInfo"
                        }
                    },
                    "nextToken": {
                        "type": "string"
                    }
                },
                "required": [
                    "items"
                ]
            },
            "NotFoundErrorResponseContent": {
                "type": "object",
                "description": "Not found error.",
                "properties": {
                    "message": {
                        "type": "string"
                    }
                },
                "required": [
                    "message"
                ]
            },
            "QueryDatasetRequestContent": {
                "type": "object",
                "properties": {
                    "sql": {
                        "type": "string"
                    }
                },
                "required": [
                    "sql"
                ]
            },
            "QueryDatasetResponseContent": {
                "type": "object",
                "properties": {
                    "data": {
                        "type": "string",
                        "contentEncoding": "byte"
                    }
                },
                "required": [
                    "data"
                ]
            },
            "SampleDatasetResponseContent": {
                "type": "object",
                "properties": {
                    "data": {
                        "type": "string",
